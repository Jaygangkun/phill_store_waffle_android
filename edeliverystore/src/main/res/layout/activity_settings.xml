<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/llSettings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/include_toolbar" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_medium_margin"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding">

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/inputLayoutAddress"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:theme="@style/CustomTextInputLayout">

                    <com.edelivery.store.widgets.CustomInputEditText
                        android:id="@+id/etAddress"
                        style="@style/CustomEditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:enabled="false"
                        android:focusableInTouchMode="false"
                        android:hint="@string/text_address"
                        android:inputType="textCapSentences"
                        android:textAlignment="viewStart"
                        app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                </com.google.android.material.textfield.TextInputLayout>

                <ImageView
                    android:id="@+id/ivStoreLocation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/activity_horizontal_padding"
                    android:paddingEnd="@dimen/activity_horizontal_padding"
                    app:srcCompat="@drawable/ic_store_location" />
            </LinearLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_top_margin"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:theme="@style/CustomTextInputLayout"
                android:visibility="gone">

                <com.edelivery.store.widgets.CustomInputEditText
                    android:id="@+id/etLat"
                    style="@style/CustomEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:hint="@string/text_lat"
                    android:inputType="numberSigned"

                    android:textAlignment="viewStart"
                    app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_top_margin"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:theme="@style/CustomTextInputLayout"
                android:visibility="gone">

                <com.edelivery.store.widgets.CustomInputEditText
                    android:id="@+id/etLng"
                    style="@style/CustomEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:hint="@string/text_lng"
                    android:inputType="numberSigned"

                    android:textAlignment="viewStart"
                    app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
            </com.google.android.material.textfield.TextInputLayout>


            <LinearLayout
                android:id="@+id/llOptionalField"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_top_margin"
                android:orientation="vertical"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding">

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/inputLayoutSlogan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:theme="@style/CustomTextInputLayout">

                    <com.edelivery.store.widgets.CustomInputEditText
                        android:id="@+id/etSlogan"
                        style="@style/CustomEditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/text_slogan"
                        android:inputType="textCapSentences"

                        android:textAlignment="viewStart"
                        app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/inputLayoutWebsite"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/general_top_margin"
                    android:theme="@style/CustomTextInputLayout">

                    <com.edelivery.store.widgets.CustomInputEditText
                        android:id="@+id/etWebsite"
                        style="@style/CustomEditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/text_website"
                        android:inputType="text"

                        android:textAlignment="viewStart"
                        app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                </com.google.android.material.textfield.TextInputLayout>

            </LinearLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_top_margin"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:theme="@style/CustomTextInputLayout">

                <com.edelivery.store.widgets.CustomInputEditText
                    android:id="@+id/etTax"
                    style="@style/CustomEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:digits="0123456789."
                    android:hint="@string/text_tax_on_order_bill"
                    android:inputType="numberDecimal"
                    android:textAlignment="viewStart"
                    app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_top_margin"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:theme="@style/CustomTextInputLayout"
                android:visibility="gone">

                <com.edelivery.store.widgets.CustomInputEditText
                    android:id="@+id/etMaxQuantity"
                    style="@style/CustomEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:focusableInTouchMode="false"
                    android:hint="@string/text_max_item_quantity_add_by_user"
                    android:inputType="number"
                    android:textAlignment="viewStart"
                    app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_top_margin"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:theme="@style/CustomTextInputLayout">

                <com.edelivery.store.widgets.CustomInputEditText
                    android:id="@+id/etMinimumOrderPrice"

                    style="@style/CustomEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:digits="0123456789."
                    android:enabled="false"
                    android:focusableInTouchMode="false"
                    android:hint="@string/text_minimum_order_price"
                    android:inputType="numberDecimal"
                    android:textAlignment="viewStart"
                    app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_top_margin"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:theme="@style/CustomTextInputLayout">

                <com.edelivery.store.widgets.CustomInputEditText
                    android:id="@+id/etDeliveryTime"
                    style="@style/CustomEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:focusableInTouchMode="false"
                    android:hint="@string/text_delivery_time"
                    android:inputType="number"
                    android:textAlignment="viewStart"
                    app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_top_margin"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:theme="@style/CustomTextInputLayout">

                <com.edelivery.store.widgets.CustomInputEditText
                    android:id="@+id/etDeliveryTimeMax"
                    style="@style/CustomEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:focusableInTouchMode="false"
                    android:hint="@string/text_delivery_time_max"
                    android:inputType="number"
                    android:textAlignment="viewStart"
                    app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
            </com.google.android.material.textfield.TextInputLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/general_top_margin"
                android:orientation="horizontal"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingEnd="@dimen/activity_horizontal_padding">

                <com.edelivery.store.widgets.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_price_rating"
                    android:textColor="@color/color_app_label"
                    android:textSize="@dimen/size_app_text_regular"
                    app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />

                <com.edelivery.store.widgets.CustomTextView
                    android:id="@+id/tvCurrency"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dimen_app_edit_text_padding"
                    android:text="($)"
                    android:textColor="@color/color_app_label"
                    android:textSize="@dimen/size_app_text_regular"
                    app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
            </LinearLayout>


            <Spinner
                android:id="@+id/spinnerPriceRatting"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:popupBackground="@color/colorWhite"
                android:prompt="@string/text_prompt_select"></Spinner>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/activity_horizontal_padding"
                android:orientation="horizontal"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingBottom="@dimen/activity_horizontal_padding">

                <com.edelivery.store.widgets.CustomTextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="@string/text_is_business"
                    android:textColor="@color/color_app_label"
                    android:textSize="@dimen/size_app_text_regular" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/switchBusiness"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/dimen_app_edit_text_padding"
                    android:theme="@style/CustomSwitch" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/activity_horizontal_padding"
                android:orientation="horizontal"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingBottom="@dimen/activity_horizontal_padding">

                <com.edelivery.store.widgets.CustomTextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="@string/text_is_busy"
                    android:textColor="@color/color_app_label"
                    android:textSize="@dimen/size_app_text_regular" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/switchBusy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/dimen_app_edit_text_padding"
                    android:theme="@style/CustomSwitch" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/activity_horizontal_padding"
                android:orientation="horizontal"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingBottom="@dimen/activity_horizontal_padding">

                <com.edelivery.store.widgets.CustomTextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="@string/text_is_estimated_time"
                    android:textColor="@color/color_app_label"
                    android:textSize="@dimen/size_app_text_regular" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/switchAskEstimatedTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/dimen_app_edit_text_padding"
                    android:theme="@style/CustomSwitch" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/activity_horizontal_padding"
                android:orientation="horizontal"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingBottom="@dimen/activity_horizontal_padding">

                <com.edelivery.store.widgets.CustomTextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="@string/text_provide_pickup_delivery"
                    android:textColor="@color/color_app_label"
                    android:textSize="@dimen/size_app_text_regular" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/switchProvidePickupDelivery"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/dimen_app_edit_text_padding"
                    android:theme="@style/CustomSwitch" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/activity_horizontal_padding"
                android:orientation="horizontal"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingBottom="@dimen/activity_horizontal_padding">

                <com.edelivery.store.widgets.CustomTextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="@string/text_is_use_item_tax"
                    android:textColor="@color/color_app_label"
                    android:textSize="@dimen/size_app_text_regular" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/switchIsUseItemTax"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/dimen_app_edit_text_padding"
                    android:theme="@style/CustomSwitch" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/activity_horizontal_padding"
                android:orientation="horizontal"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingBottom="@dimen/activity_horizontal_padding">

                <com.edelivery.store.widgets.CustomTextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="@string/text_delivery_store_time"
                    android:textColor="@color/color_app_label"
                    android:textSize="@dimen/size_app_text_regular" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/switchIsSetDeliveryTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/dimen_app_edit_text_padding"
                    android:theme="@style/CustomSwitch" />
            </LinearLayout>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_marginStart="@dimen/activity_horizontal_padding"
                android:layout_marginTop="@dimen/activity_horizontal_padding"
                android:layout_marginEnd="@dimen/activity_horizontal_padding">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="@dimen/dimen_app_edit_text_padding">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/dimen_app_edit_text_padding"
                        android:paddingBottom="@dimen/dimen_app_edit_text_padding">

                        <com.edelivery.store.widgets.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:text="@string/text_pay_delivery_fee"
                            android:textColor="@color/color_app_label"
                            android:textSize="@dimen/size_app_text_regular" />

                        <androidx.appcompat.widget.SwitchCompat
                            android:id="@+id/switchPayDeliveryFees"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:theme="@style/CustomSwitch" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/llPayDeliveryFees"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dimen_expand_penal_content_2"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/general_top_margin"
                            android:theme="@style/CustomTextInputLayout">

                            <com.edelivery.store.widgets.CustomInputEditText
                                android:id="@+id/etFreeDeliveryPrice"
                                style="@style/CustomEditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:digits="0123456789."
                                android:hint="@string/text_free_delivery_for_above_price"
                                android:inputType="numberDecimal"
                                android:textAlignment="viewStart"
                                app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/general_top_margin"
                            android:theme="@style/CustomTextInputLayout">

                            <com.edelivery.store.widgets.CustomInputEditText
                                android:id="@+id/etFreeDeliveryRadius"
                                style="@style/CustomEditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:digits="0123456789."
                                android:enabled="false"
                                android:focusableInTouchMode="false"
                                android:hint="@string/text_free_delivery_radius"
                                android:imeOptions="actionDone"
                                android:inputType="numberDecimal"
                                android:textAlignment="viewStart"
                                app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvCancellationCharge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/activity_horizontal_padding"
                android:layout_marginTop="@dimen/activity_horizontal_padding"
                android:layout_marginEnd="@dimen/activity_horizontal_padding">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="@dimen/dimen_app_edit_text_padding">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen_app_edit_text_padding"
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/dimen_app_edit_text_padding"
                        android:paddingBottom="@dimen/dimen_app_edit_text_padding">

                        <com.edelivery.store.widgets.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:text="@string/text_order_cancellation_charge"
                            android:textColor="@color/color_app_label"
                            android:textSize="@dimen/size_app_text_regular" />

                        <androidx.appcompat.widget.SwitchCompat
                            android:id="@+id/switchOrderCancellationCharge"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:enabled="false"
                            android:theme="@style/CustomSwitch" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/llCancellationCharge"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dimen_expand_penal_content_3"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/general_top_margin"
                            android:theme="@style/CustomTextInputLayout">

                            <com.edelivery.store.widgets.CustomInputEditText
                                android:id="@+id/etCancellationChargeAbovePrice"
                                style="@style/CustomEditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:digits="0123456789."
                                android:enabled="false"
                                android:focusableInTouchMode="false"
                                android:hint="@string/text_order_cancellation_charge_above_order_price"
                                android:inputType="numberDecimal"
                                android:textAlignment="viewStart"
                                app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/general_top_margin"
                            android:theme="@style/CustomTextInputLayout">

                            <com.edelivery.store.widgets.CustomInputEditText
                                android:id="@+id/etCancellationChargeValue"
                                style="@style/CustomEditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:digits="0123456789."
                                android:enabled="false"
                                android:focusableInTouchMode="false"
                                android:hint="@string/text_order_cancellation_charge_value"
                                android:inputType="numberDecimal"
                                android:textAlignment="viewStart"
                                app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                        </com.google.android.material.textfield.TextInputLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimen_app_edit_text_padding"
                            android:layout_marginTop="@dimen/general_top_margin"
                            android:orientation="horizontal">

                            <com.edelivery.store.widgets.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/text_order_cancellation_charge_type"
                                android:textColor="@color/color_app_label"
                                android:textSize="@dimen/size_app_text_regular"
                                app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />

                            <com.edelivery.store.widgets.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dimen_app_edit_text_padding"
                                android:text="($)"
                                android:textColor="@color/color_app_label"
                                android:textSize="@dimen/size_app_text_regular"
                                android:visibility="gone"
                                app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                        </LinearLayout>

                        <Spinner
                            android:id="@+id/spinnerChargeType"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:popupBackground="@color/colorWhite"
                            android:prompt="@string/text_prompt_select"></Spinner>
                    </LinearLayout>

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_marginStart="@dimen/activity_horizontal_padding"
                android:layout_marginTop="@dimen/activity_horizontal_padding"
                android:layout_marginEnd="@dimen/activity_horizontal_padding">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="@dimen/dimen_app_edit_text_padding">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingTop="@dimen/dimen_app_edit_text_padding"
                        android:paddingBottom="@dimen/dimen_app_edit_text_padding">

                        <com.edelivery.store.widgets.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:text="@string/text_provide_delivery_anywhere"
                            android:textColor="@color/color_app_label"
                            android:textSize="@dimen/size_app_text_regular" />

                        <androidx.appcompat.widget.SwitchCompat
                            android:id="@+id/switchProviderDeliveryAnywhere"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:enabled="false"
                            android:theme="@style/CustomSwitch" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/llDeliveryAnyWhere"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dimen_expand_penal_content_1"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/general_top_margin"
                            android:theme="@style/CustomTextInputLayout">

                            <com.edelivery.store.widgets.CustomInputEditText
                                android:id="@+id/etDeliveryRadius"
                                style="@style/CustomEditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:digits="0123456789."
                                android:hint="@string/text_delivery_radius"
                                android:inputType="numberDecimal"
                                android:textAlignment="viewStart"
                                app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                        </com.google.android.material.textfield.TextInputLayout>

                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/activity_horizontal_padding"
                android:layout_marginTop="@dimen/activity_horizontal_padding"
                android:layout_marginEnd="@dimen/activity_horizontal_padding">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="@dimen/dimen_app_edit_text_padding">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen_app_edit_text_padding"
                        android:orientation="horizontal"

                        android:paddingTop="@dimen/dimen_app_edit_text_padding"
                        android:paddingBottom="@dimen/dimen_app_edit_text_padding">

                        <com.edelivery.store.widgets.CustomTextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:text="@string/text_taking_schedule_order"
                            android:textColor="@color/color_app_label"
                            android:textSize="@dimen/size_app_text_regular" />

                        <androidx.appcompat.widget.SwitchCompat
                            android:id="@+id/switchTakingScheduleOrder"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:enabled="false"
                            android:theme="@style/CustomSwitch" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/llScheduleOrder"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dimen_expand_penal_content_1"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/general_top_margin"
                            android:theme="@style/CustomTextInputLayout">

                            <com.edelivery.store.widgets.CustomInputEditText
                                android:id="@+id/etScheduleOrderCreateAfterMinute"
                                style="@style/CustomEditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:enabled="false"
                                android:focusableInTouchMode="false"
                                android:hint="@string/text_schedule_order_create_after_minute"
                                android:inputType="number"
                                android:textAlignment="viewStart"
                                app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/general_top_margin"
                            android:theme="@style/CustomTextInputLayout"
                            android:visibility="gone">

                            <com.edelivery.store.widgets.CustomInputEditText
                                android:id="@+id/etInformScheduleOrderBeforeMinute"
                                style="@style/CustomEditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:enabled="false"
                                android:focusableInTouchMode="false"
                                android:hint="@string/text_inform_schedule_order_before_minute"
                                android:inputType="number"
                                android:textAlignment="viewStart"
                                app:typefaceInputEt="fonts/AvenirLTStd-Medium.otf" />
                        </com.google.android.material.textfield.TextInputLayout>


                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <com.edelivery.store.widgets.CustomTextView
                android:id="@+id/tvAddNewTag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/activity_horizontal_padding"
                android:drawablePadding="@dimen/activity_horizontal_padding"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:paddingBottom="@dimen/activity_horizontal_padding"
                android:text="@string/text_famous_for"
                android:textColor="@color/color_app_heading"
                android:textSize="@dimen/size_app_button_text"
                android:visibility="visible"
                app:drawableLeftCompat="@drawable/ic_famous_for" />


            <com.edelivery.store.widgets.CustomTextView
                android:id="@+id/tvAddNewStoreTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawablePadding="@dimen/activity_horizontal_padding"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:paddingBottom="@dimen/activity_horizontal_padding"
                android:text="@string/text_add_new_store_time"
                android:textColor="@color/color_app_heading"
                android:textSize="@dimen/size_app_button_text"
                android:visibility="visible"
                app:drawableLeftCompat="@drawable/ic_store_time" />

            <com.edelivery.store.widgets.CustomTextView
                android:id="@+id/tvAddNewStoreDeliverySlot"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/activity_horizontal_padding"
                android:drawablePadding="@dimen/activity_horizontal_padding"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:text="@string/text_delivery_store_time"
                android:textColor="@color/color_app_heading"
                android:textSize="@dimen/size_app_button_text"
                android:visibility="visible"
                app:drawableLeftCompat="@drawable/store_delivery_time" />


            <com.edelivery.store.widgets.CustomTextView
                android:id="@+id/tvSelectLanguages"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/activity_horizontal_padding"
                android:drawablePadding="@dimen/activity_horizontal_padding"
                android:paddingStart="@dimen/activity_horizontal_padding"
                android:paddingTop="@dimen/activity_horizontal_padding"
                android:paddingEnd="@dimen/activity_horizontal_padding"
                android:paddingBottom="@dimen/activity_horizontal_padding"
                android:text="@string/text_select_language"
                android:textColor="@color/color_app_heading"
                android:textSize="@dimen/size_app_button_text"
                android:visibility="visible"
                app:drawableLeftCompat="@drawable/ic_icon_language" />
        </LinearLayout>


    </androidx.core.widget.NestedScrollView>
</LinearLayout>